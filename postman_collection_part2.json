{
    "name": "Feeds",
    "description": "Feed management and interaction endpoints",
    "item": [
        {
            "name": "Get All Feeds",
            "request": {
                "method": "GET",
                "url": {
                    "raw": "{{base_url}}/api/feeds",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "feeds"
                    ]
                },
                "description": "Get a list of all public feeds."
            }
        },
        {
            "name": "Search Feeds",
            "request": {
                "method": "GET",
                "url": {
                    "raw": "{{base_url}}/api/feeds/search?query=project",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "feeds",
                        "search"
                    ],
                    "query": [
                        {
                            "key": "query",
                            "value": "project"
                        }
                    ]
                },
                "description": "Search for feeds by title or content."
            }
        },
        {
            "name": "Get Feeds by Author",
            "request": {
                "method": "GET",
                "url": {
                    "raw": "{{base_url}}/api/feeds/author/123",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "feeds",
                        "author",
                        "123"
                    ]
                },
                "description": "Get all feeds created by a specific user."
            }
        },
        {
            "name": "Get Feed by ID",
            "request": {
                "method": "GET",
                "url": {
                    "raw": "{{base_url}}/api/feeds/123",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "feeds",
                        "123"
                    ]
                },
                "description": "Get detailed information about a specific feed."
            }
        },
        {
            "name": "Create Feed",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{access_token}}"
                    },
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n    \"title\": \"New Project Launch\",\n    \"content\": \"We're excited to announce the launch of our new project!\"\n}"
                },
                "url": {
                    "raw": "{{base_url}}/api/feeds",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "feeds"
                    ]
                },
                "description": "Create a new feed post. Requires authentication."
            }
        },
        {
            "name": "Update Feed",
            "request": {
                "method": "PUT",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{access_token}}"
                    },
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n    \"title\": \"Updated Project Launch\",\n    \"content\": \"We're excited to announce the launch of our new project with additional features!\"\n}"
                },
                "url": {
                    "raw": "{{base_url}}/api/feeds/123",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "feeds",
                        "123"
                    ]
                },
                "description": "Update an existing feed post. Requires authentication and ownership."
            }
        },
        {
            "name": "Delete Feed",
            "request": {
                "method": "DELETE",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{access_token}}"
                    }
                ],
                "url": {
                    "raw": "{{base_url}}/api/feeds/123",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "feeds",
                        "123"
                    ]
                },
                "description": "Delete a feed post. Requires authentication and ownership."
            }
        }
    ]
}